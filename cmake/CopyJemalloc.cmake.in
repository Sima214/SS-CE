# Find all the libraries.
file( GLOB JEMALLOC_LINKED_LIBS "@CMAKE_BINARY_DIR@/jemalloc/lib/*.@DYNLIB_EXT@*" )
# Strip libraries.
if( NOT "@CMAKE_STRIP@" STREQUAL "" )
    foreach(lib ${JEMALLOC_LINKED_LIBS})
        execute_process( COMMAND @CMAKE_STRIP@ ${lib} )
    endforeach()
endif()
# Install libraries.
file( INSTALL ${JEMALLOC_LINKED_LIBS} DESTINATION "@SSCE_INSTALL_DIR@" )